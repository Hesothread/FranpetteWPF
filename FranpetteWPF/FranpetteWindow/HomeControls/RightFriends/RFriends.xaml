<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="FranpetteWPF.FranpetteWindow.HomeControls.RightFriends.RFriends" 
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="250">
    <Grid x:Name="RFriendsGrid" Background="{StaticResource DarkGray}">
        <ListBox Height="auto" Width="auto" Background="Transparent" ItemsSource="{Binding FriendList}"/>
    </Grid>
</UserControl>
